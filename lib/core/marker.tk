// lib/core/marker.tk
// Core compiler traits and markers.

pub trait @Drop {
    fn drop(self)
}

// Marker trait for types that can be safely moved.
pub trait @Move {}

// Trait for types that can create a copy of themselves.
pub trait @Clone {
    pub fn clone(self)
}

// Trait for types that have a default value.
pub trait @Default {
    pub fn init(self)
}

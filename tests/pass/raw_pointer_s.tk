import std/memory

shape Hero (
    *buf#: i32
)

impl Hero@encap {
    pub *
    pub fn clone(self) -> Hero {
        return Hero(*buf = self.*buf)
    }
    fn drop(self#) {
        
    }
}

fn main() -> i32 {

    unsafe {
        auto *p# = alloc [10]i32(42)
        p[5] = 5
        if p[5] != 5 {
            return -1
        }
        auto *q# = *p[6]
        q = 6
        if p[6] != 6 {
            return -1
        }
    }
    unsafe {
        auto *p# = alloc [10]i32(42)
        auto h# = Hero(*buf# = *p)
        h.buf[0] = 10
        if p[0] != 10 {
            return -1
        }
    }
    return 0
}
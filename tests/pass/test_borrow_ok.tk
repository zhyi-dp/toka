import core/io::println

fn main() -> i32 {
    auto x# = 10
    
    {
        auto &y1 = &x
        auto &y2 = &x
        println("y1={}, y2={}", y1, y2)
    }
    
    // After y1, y2 go out of scope, we can mutably borrow
    {
        auto &z# = &x#
        z# = 20
        println("z={}", z)
        // println("x={}", x) // This would be an error
    }
    
    println("final x={}", x)
    return 0
}

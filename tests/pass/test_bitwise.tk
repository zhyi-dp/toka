fn main() -> i32 {
    auto a = 12
    auto b = 10
    
    // Test band: 1100 & 1010 = 1000 (8)
    auto c = a band b
    if c != 8 {
        return 1
    }

    // Test bor: 1100 | 1010 = 1110 (14)
    auto d = a bor b
    if d != 14 {
        return 2
    }

    // Test bxor: 1100 ^ 1010 = 0110 (6)
    auto e = a bxor b
    if e != 6 {
        return 3
    }

    // Test bnot: ~12 = -13 (in 2s complement)
    auto f = bnot a
    if f != -13 {
        return 4
    }

    // Test simple precedence with parens (User style)
    // (1 bor 2) band 4 = 3 band 4 = 0
    auto g = (1 bor 2) band 4
    if g != 0 {
        return 5
    }

    // Test without parens? 
    // Precedence: band (10) > bor (5)
    // 1 bor 2 band 4 = 1 bor (2 band 4) = 1 bor 0 = 1
    auto h = 1 bor 2 band 4
    if h != 1 { 
        return 6 
    }

    // Test bshl: 1 << 2 = 4 (High precedence)
    auto i = 1 bshl 2
    if i != 4 {
        return 7
    }

    // Test bshr: 4 >> 1 = 2 (High precedence)
    auto j = 4 bshr 1
    if j != 2 {
        return 8
    }

    // Test precedence: 1 bshl 2 band 6
    // (1 << 2) & 6 = 4 & 6 = 4 (bshl is higher than band)
    // If equal: 1 << (2 & 6) = 1 << 2 = 4. Bad example.
    // 1 bshl 2 bor 1
    // (1 << 2) | 1 = 4 | 1 = 5.
    // 1 << (2 | 1) = 1 << 3 = 8.
    auto k = 1 bshl 2 bor 1
    if k != 5 {
        return 9
    }

    return 0
}

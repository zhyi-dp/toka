import core/io::printf

fn main() -> i32 {
    auto x# = 10
    
    {
        auto &y1 = &x
        auto &y2 = &x
        io::printf("y1=%d, y2=%d\n", y1, y2)
    }
    
    // After y1, y2 go out of scope, we can mutably borrow
    {
        auto &z# = &x#
        z# = 20
        io::printf("z=%d\n", z)
        // io::printf("x=%d\n", x); // This would be an error
    }
    
    io::printf("final x=%d\n", x)
    return 0
}

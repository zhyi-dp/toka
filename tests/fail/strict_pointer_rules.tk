
shape Point(x: i32, y: i32)

fn main() -> i32 {
    unsafe {
        // FAIL 1: Implicit pointer inference
        auto p1 = new Point(x=1, y=2) 
        // EXPECT: E0451
        
        auto ^p2# = new Point(x=3, y=4)
        
        // FAIL 2: Dot with sigil
        (^p2).x = 10
        // EXPECT: E0452
        
        // FAIL 3: Arrow without sigil
        p2->y = 20
        // EXPECT: E0453
    }
    return 0
}
